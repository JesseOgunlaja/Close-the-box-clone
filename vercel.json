{
  "version": 2,
  "builds": [
    {
      "src": "web/main.dart",
      "use": "@vercel/static-build",
      "config": { "distDir": "web" },
      "build": {
        "env": { "DART-SASS": "1" },
        "commands": {
          "build": "dart compile js ./web/main.dart -o web/main.dart.js"
        }
      }
    }
  ]
}
